<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/noteback">

    <TextView
            android:text="@string/DawnNote"
            android:fontFamily="@font/javatext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView1"
            android:textSize="40sp"
            android:layout_gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.01999998" android:textColor="#FFFFFF"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.481"
            android:layout_marginLeft="20dp" android:layout_marginStart="20dp"/>
    <TextView
            android:text="@string/copyright"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            android:layout_gravity="center" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:textColor="#FFFFFF"
    />
    <TextView
            android:text="확통1"
            android:fontFamily="@font/korean"
            android:layout_gravity="center"
            android:textColor="#FFFFFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/name1"
            app:layout_constraintTop_toBottomOf="@+id/imageView3"
            app:layout_constraintStart_toStartOf="@+id/imageView3"
            app:layout_constraintEnd_toEndOf="@+id/imageView3"
    />
    <TextView
            android:text="확통2"
            android:fontFamily="@font/korean"
            android:layout_gravity="center"
            android:textColor="#FFFFFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/name2"
            app:layout_constraintTop_toBottomOf="@+id/imageView4"
            app:layout_constraintStart_toStartOf="@+id/imageView4" app:layout_constraintEnd_toEndOf="@+id/imageView4"/>
    <TextView
            android:text="확통3"
            android:fontFamily="@font/korean"
            android:layout_gravity="center"
            android:textColor="#FFFFFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/name3"
            app:layout_constraintTop_toBottomOf="@+id/imageView5"
            app:layout_constraintStart_toStartOf="@+id/imageView5" app:layout_constraintEnd_toEndOf="@+id/imageView5"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="49dp"
            app:srcCompat="@drawable/moon"
            android:id="@+id/imageView"

            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.225"
            app:layout_constraintVertical_bias="0.032"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/detective" android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_gravity="center"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.933"
            app:layout_constraintVertical_bias="0.037"/>

    <ImageView
            android:layout_width="200dp"
            android:layout_height="200dp"
            app:srcCompat="@drawable/st1"
            android:id="@+id/imageView3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.25" android:layout_marginLeft="17dp"
            android:layout_marginStart="17dp" android:layout_marginTop="60dp"/>
    <ImageButton
            android:layout_width="150dp"
            android:layout_height="200dp" app:srcCompat="@drawable/st2" android:id="@+id/imageView4"
            android:layout_gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="60dp"
            app:layout_constraintVertical_bias="0.25" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="200dp"
            android:layout_height="200dp" app:srcCompat="@drawable/st3" android:id="@+id/imageView5"
            android:layout_gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.256"
            app:layout_constraintBottom_toTopOf="@+id/textView3" android:layout_marginEnd="17dp"
            android:layout_marginRight="17dp" android:layout_marginTop="60dp"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp" app:srcCompat="@drawable/smallfolder" android:id="@+id/imageView6"
            android:layout_weight="1"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.348" app:layout_constraintVertical_bias="0.187"
            android:layout_marginRight="40dp" android:layout_marginEnd="40dp"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/pinkplus_resize"
            android:id="@+id/imageButton2"
            android:background="@drawable/round"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.932" app:layout_constraintVertical_bias="0.93"/>
    <TextView
            android:text=".../ 중간고사/확률 및 통계"
            android:fontFamily="@font/korean" android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView16"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:textColorLink="#FFFFFF"
            android:textColor="#FFFFFF" app:layout_constraintVertical_bias="0.193" android:textSize="16sp"
            app:layout_constraintHorizontal_bias="0.499"/>
    <TextView
            android:text="Note"
            android:fontFamily="@font/javatext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            android:textColor="#FFFFFF" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.110000014" android:textSize="24sp"
            app:layout_constraintEnd_toStartOf="@+id/imageView9" app:layout_constraintHorizontal_bias="0.85"/>
    <TextView
            android:text="Tag"
            android:fontFamily="@font/javatext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            android:textColor="#FFFFFF" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.110000014" android:textSize="24sp"
            app:layout_constraintStart_toEndOf="@+id/imageView9" app:layout_constraintHorizontal_bias="0.15"/>
    <ImageView
            android:layout_width="49dp"
            android:layout_height="58dp" app:srcCompat="@drawable/back" android:id="@+id/imageView8"
            tools:layout_editor_absoluteX="16dp" tools:layout_editor_absoluteY="29dp"
            tools:ignore="MissingConstraints"/>

    <ImageView
            android:layout_width="160dp"
            android:layout_height="50dp"
            app:srcCompat="@drawable/aa"
            android:id="@+id/imageView9"
            android:layout_gravity="center"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.110000014"/>
    <Button

            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/pink"
            android:id="@+id/button6"
            android:layout_gravity="left"

            app:layout_constraintTop_toTopOf="@+id/imageView9"
            android:layout_marginTop="9dp" app:layout_constraintEnd_toEndOf="@+id/imageView9"
            app:layout_constraintStart_toStartOf="@+id/imageView9"
            app:layout_constraintBottom_toBottomOf="@+id/imageView9" app:layout_constraintVertical_bias="1.0"
            app:layout_constraintHorizontal_bias="0.3"/>

</androidx.constraintlayout.widget.ConstraintLayout>